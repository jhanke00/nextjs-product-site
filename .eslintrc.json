{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off", 
    "quotes": ["error", "single"],
    "semi": ["error", "always"], 
    "indent": ["error", 2], 
    "react/prop-types": "off" 
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
